{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug_startup.ts",
        "type": "pwa-node",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "runtimeArgs": [
          "-r", "ts-node/register",
          "-r", "source-map-support/register"
        ],
        "args": [
          "${workspaceFolder}/src/startup.ts"
        ],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "skipFiles": ["<node_internals>/**"],
        "sourceMaps": true
      }
    ]
  }